{
    "name": "Threat Intelligence Feeds - Increases security significantly! (Recommended)",
    "description": "A blocklist for blocking Malware, Cryptojacking, Scam, Spam and Phishing. Blocks domains known to spread malware, launch phishing attacks and host command-and-control servers.",
    "homepage": "https://github.com/hagezi/dns-blocklists",
    "tags": ["malware", "cryptojacking", "scam", "spam", "phishing", "security"],
    "mainMirror": "https://cdn.jsdelivr.net/gh/hagezi/dns-blocklists@latest/hosts/tif.txt",
    "alternativeMirrors": [
        "https://gitlab.com/hagezi/mirror/-/raw/main/dns-blocklists/hosts/tif.txt",
        "https://codeberg.org/hagezi/mirror2/raw/branch/main/dns-blocklists/hosts/tif.txt"
    ],
    "detectionMethods": {
        "title": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:Title|Name):\\s*(.*)$"
            ],
            "priority": 1
        },
        "description": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:Description|Desc):\\s*(.*)$"
            ],
            "priority": 2
        },
        "homepage": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:Homepage|URL):\\s*(.*)$"
            ],
            "priority": 3
        },
        "license": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:License|Lic):\\s*(.*)$"
            ],
            "priority": 4
        },
        "expiry": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:Expires?|Expiry|Expiration):\\s*(.*)$"
            ],
            "priority": 5
        },
        "lastModified": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:Last modified|Modified|Updated):\\s*(.*)$"
            ],
            "priority": 6
        },
        "version": {
            "type": "text-locate",
            "patterns": [
                "^#\\s*(?:Version|Ver):\\s*(.*)$"
            ],
            "priority": 7
        }
    }
}